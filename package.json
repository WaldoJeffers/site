{
  "name": "styleguidist-site",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me"
  },
  "homepage": "https://github.com/styleguidist/site",
  "repository": "styleguidist/site",
  "license": "See license in <Readme.md>",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "pretest": "npm run lint && npm run lint:css",
    "lint": "eslint . --fix --ext .js,.jsx",
    "lint:css": "stylelint --fix '**/*.pcss'",
    "start": "tamia server & npm run build:watch",
    "test": "npm run lint:css && npm run lint:js",
    "sync": "./scripts/sync.sh",
    "deploy": "./scripts/deploy.sh",
    "bundle": "tamia bundle",
    "prebuild": "mkdir -p public/build && touch public/build/styles.css",
    "build": "node src",
    "build:watch": "chokidar source 'templates/**/*.jsx' -c 'node src'"
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "tamia": "^3.0.0-aplha.12"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chokidar-cli": "^1.2.0",
    "classnames": "^2.2.5",
    "eslint": "^3.19.0",
    "eslint-config-tamia": "^3.3.4",
    "eslint-plugin-react": "^6.10.3",
    "fledermaus": "^7.0.0",
    "github-slugger": "^1.1.3",
    "lodash": "^4.17.4",
    "stylelint": "^7.11.1",
    "stylelint-config-standard": "^16.0.0",
    "tamia-build": "^1.2.7",
    "unist-util-visit": "^1.1.3"
  }
}
